{
  "name": "docs",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "build-openapi-docs": "cp openapi/openapi.yaml public/sdks/openapi/openapi.yaml",
    "build-javadoc": "rm -rf public/sdks/java && cd ../ && ./gradlew dokkaJavadocCollector && cp -r ./build/docs/dokka/javadoc docs/public/sdks/java",
    "build-kdoc": "rm -rf public/sdks/kotlin && cd ../ && ./gradlew dokkaHtmlMultiModule && cp -r ./build/dokka/htmlMultiModule docs/public/sdks/kotlin",
    "build-clj-docs": "rm -rf public/sdks/clojure && cd ../ && ./gradlew build-codox"
  },
  "dependencies": {
    "@astrojs/sitemap": "3.0.3",
    "@astrojs/starlight": "^0.15.0",
    "@astrojs/starlight-tailwind": "^2.0.1",
    "@astrojs/tailwind": "^5.0.3",
    "astro": "^4.0.1",
    "date-fns": "^2.29.3",
    "rehype-highlight": "^6.0.0",
    "sharp": "^0.32.5",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@rollup/plugin-yaml": "^4.0.1",
    "@tailwindcss/typography": "^0.5.9",
    "asciidoctor": "^2.2.6"
  }
}
